<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
    <title>LawyerJee.com</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/themes/base/css")
    @*@Styles.Render("~/Content/themes/base/css")*@
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @*@Scripts.Render("~/bundles/jqueryselection")*@
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/bootstrap")   
    @Scripts.Render("~/bundles/customjs")


</head>
<body>
    <div class="header_main">
        <div class="container">
            <div class="header_in">
                <div class="logo_main">
                    @*@Html.ActionLink("Lawyerjee.com", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })*@
                    <a href="#" onclick="location.href='@Url.Action("Index","Home")'"><img src="~/images/logo.png"></a>
                </div>
                <div class="right_menu">
                    @Html.Partial("_LoginPartial")
                </div>
            </div>
        </div>
    </div>
  
    <div>
        @RenderBody()

        <footer>
            <div class="container">
                <div>
                    <div class="col-md-6 col-sm-6">
                        <p>© 2015 lawyer.com develop by xyz</p>
                    </div>
                    <div class="col-md-6 col-sm-6">
                        <ul class="social_main">
                            <li><a class="fa fa-twitter" title="twitter" target="_blank" href="#">&nbsp;</a></li>
                            <li><a class="fa fa-facebook" title="facebook" target="_blank" href="#">&nbsp;</a></li>
                            <li><a class="fa fa-linkedin-square" title="linkedin" target="_blank" href="#">&nbsp;</a></li>
                            <li><a class="fa fa-pinterest" title="pinterest" target="_blank" href="#">&nbsp;</a></li>
                            <li><a class="fa fa-instagram" title="instagram" target="_blank" href="#">&nbsp;</a></li>
                            <li><a class="fa fa-youtube" title="youtube" target="_blank" href="#">&nbsp;</a></li>
                        </ul>
                    </div>
                </div>
            </div>
        </footer>
    </div>
  


    @RenderSection("scripts", required: false)
</body>
</html>
